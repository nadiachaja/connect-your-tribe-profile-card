{% render 'partials/head.liquid' %}

<section>
  <div>
    <img class="profile" src="assets/foto-ik.svg" alt="">
    <h1>{{ person.name }}</h1>

    <div class="sub-info">
    <p class="birth-year">{{person.custom.year}}</p>
  
    <p class="age">{{person.custom.age}}</p>
    
    <p class="education">{{person.bio}}</p>
    </div>

    <details> 
      <summary>Info</summary>
      <p class="info-text">{{person.custom.info}}</p>
    </details>

    <div class="footer">
    <div class="svg">
    <img class="camera" src="assets/camera-svgrepo-com.svg" alt="camera">
    <img class="html-tag" src="assets/html-tag-svgrepo-com.svg" alt="html tag">
    <img class="book" src="assets/book-7-svgrepo-com.svg" alt="boek">
    <img class="music" src="assets/music-notes-svgrepo-com.svg" alt="muzieknoten">
    <img class="movie" src="assets/movies-and-tv-svgrepo-com.svg" alt="clapperboard">
    </div>

    <a href=""><img class="qr-code" src="assets/qrcode.71249255.png" alt=""></a>
    </div>

    </section>
  </div>
  
  <script src="scripts/script.js"></script>
{% render 'partials/foot.liquid' %}
